{
  "name": "@bitr/chronodb",
  "version": "1.0.2",
  "description": "ChronoDB is a time-series local DB for Node.js powered by Google's LevelDB.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "jest": "jest --runInBand",
    "cov": "jest --coverage --runInBand"
  },
  "keywords": [
    "time-series",
    "leveldb",
    "levelup",
    "typescript"
  ],
  "repository": "https://github.com/bitrinjani/chronodb",
  "author": "bitrinjani <bitrinjani@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "level": "^6.0.1",
    "lodash": "^4.17.4",
    "mkdirp": "^1.0.4",
    "through2": "^3.0.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/lodash": "^4.14.91",
    "@types/node": "^14.0.1",
    "@types/through2": "^2.0.33",
    "@types/uuid": "^8.0.0",
    "coveralls": "^3.0.0",
    "jest": "^25.5.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/__tests__/**",
      "!**/node_modules/**",
      "!**/*.d.ts",
      "!**/types.ts"
    ]
  }
}
